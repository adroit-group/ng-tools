{
  "version": 2,
  "projects": {
    "ng-util-demo": {
      "$schema": "..\\..\\node_modules\\nx\\schemas\\project-schema.json",
      "projectType": "application",
      "root": "apps/ng-util-demo",
      "sourceRoot": "apps/ng-util-demo/src",
      "prefix": "adroit",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/ng-util-demo",
            "index": "apps/ng-util-demo/src/index.html",
            "main": "apps/ng-util-demo/src/main.ts",
            "polyfills": "apps/ng-util-demo/src/polyfills.ts",
            "tsConfig": "apps/ng-util-demo/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/ng-util-demo/src/favicon.ico",
              "apps/ng-util-demo/src/assets"
            ],
            "styles": [
              "apps/ng-util-demo/src/styles.scss"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "statsJson": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": true,
              "buildOptimizer": true,
              "commonChunk": true,
              "deleteOutputPath": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/ng-util-demo/src/environments/environment.ts",
                  "with": "apps/ng-util-demo/src/environments/environment.prod.ts"
                }
              ]
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "ng-util-demo:build:production"
            },
            "development": {
              "browserTarget": "ng-util-demo:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ng-util-demo:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/ng-util-demo/src/**/*.ts",
              "apps/ng-util-demo/src/**/*.html"
            ]
          },
          "outputs": [
            "{options.outputFile}"
          ]
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/ng-util-demo"
          ],
          "options": {
            "jestConfig": "apps/ng-util-demo/jest.config.ts",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "ng-util-demo-e2e": {
      "$schema": "..\\..\\node_modules\\nx\\schemas\\project-schema.json",
      "root": "apps/ng-util-demo-e2e",
      "sourceRoot": "apps/ng-util-demo-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/ng-util-demo-e2e/cypress.json",
            "tsConfig": "apps/ng-util-demo-e2e/tsconfig.e2e.json",
            "devServerTarget": "ng-util-demo:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ng-util-demo:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/ng-util-demo-e2e/**/*.{js,ts}"
            ]
          },
          "outputs": [
            "{options.outputFile}"
          ]
        }
      },
      "tags": [],
      "implicitDependencies": [
        "ng-util-demo"
      ]
    },
    "ng-utils": {
      "$schema": "..\\..\\node_modules\\nx\\schemas\\project-schema.json",
      "projectType": "library",
      "root": "libs/ng-utils",
      "sourceRoot": "libs/ng-utils/src",
      "prefix": "ad",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:package",
          "outputs": [
            "dist/libs/ng-utils"
          ],
          "options": {
            "project": "libs/ng-utils/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/ng-utils/tsconfig.lib.json"
            },
            "development": {
              "tsConfig": "libs/ng-utils/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/ng-utils"
          ],
          "options": {
            "jestConfig": "libs/ng-utils/jest.config.ts",
            "passWithNoTests": false
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ng-utils/src/**/*.ts",
              "libs/ng-utils/src/**/*.html",
              "libs/ng-utils/ngx-cdps-meter/**/*.ts",
              "libs/ng-utils/ngx-cdps-meter/**/*.html"
            ]
          },
          "outputs": [
            "{options.outputFile}"
          ]
        }
      },
      "tags": []
    },
    "ngx-di-unchained": {
      "projectType": "library",
      "root": "libs/ngx-di-unchained",
      "sourceRoot": "libs/ngx-di-unchained/src",
      "prefix": "ad",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:package",
          "outputs": ["dist/libs/ngx-di-unchained"],
          "options": {
            "project": "libs/ngx-di-unchained/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/ngx-di-unchained/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/ngx-di-unchained/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ngx-di-unchained"],
          "options": {
            "jestConfig": "libs/ngx-di-unchained/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ngx-di-unchained/src/**/*.ts",
              "libs/ngx-di-unchained/src/**/*.html"
            ]
          }
        }
      },
      "tags": []
    }
  }
}
