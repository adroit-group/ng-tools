    "ts-jest": "^27.0",
    "ts-morph": "^11",
    "ts-node": "^10",
    "tslib": "^2.0.0",
    "typescript": "4.5.5",
    "webpack": "^5.51.1",
    "webpack-bundle-analyzer": "^4.4.2"
  }
