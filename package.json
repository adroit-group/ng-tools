  "repository": {
    "type": "git",
    "url": "https://github.com/Adroit-Group/ng-tools"
  },
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "nx": "nx",
    "start": "ng serve",
    "analyze-bundles": "webpack-bundle-analyzer dist/apps/ng-util-demo/stats.json",
    "build": "ng build",
    "build:app:prod": "ng build ng-utils-demo --prod",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "webpack": "webpack",
    "lib:compodoc": "npx compodoc libs/ng-utils/src -p libs/ng-utils/tsconfig.lib.json -s",
    "----------------------------------------  NG UTILS LIB  ------------------------------------------": "",
    "prebuild:lib": "rimraf dist/libs/ng-utils",
    "build:lib": "ng build ng-utils --verbose --c=production",
    "postbuild:lib": "npx ts-node tasks/correct-mixin-types.ts",
    "install:lib:local": "npm i ./dist/libs/ng-utils",
    "version-lib": "cd libs/ng-utils && npm version --no-git-tag-version",
    "postversion-lib": "cd ..\\..\\",
    "prettier:check": "prettier --check ./libs/ng-utils/src/**/*.ts",
    "validate": "run-s test:lib lint:lib",
    "prepublish:lib": "npm run validate && npm run build:lib",
    "publish:lib": "npm publish ./dist/libs/ng-utils --access public",
    "lint:lib": "nx workspace-lint && ng lint ng-utils",
    "test:lib": "ng test ng-utils",
    "test:lib:dev": "ng test ng-utils --watch=true",
    "test:lib:coverage": "ng test ng-utils --coverage",
    "test:lib:doc:coverage": "npx ts-node tasks/test-doc-coverage.ts"
  },
  "dependencies": {
