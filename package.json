{
  "name": "adroit",
  "version": "0.0.0",
  "license": "MIT",
  "private": false,
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "nx": "nx",
    "start": "ng serve",
    "analyze-bundles": "webpack-bundle-analyzer dist/apps/ng-util-demo/stats.json",
    "build": "ng build",
    "build:app:prod": "ng build ng-utils-demo --prod",
    "prebuild:lib": "rimraf dist/libs/ng-utils",
    "build:lib": "ng build ng-utils --verbose -c=production",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "webpack": "webpack",
    "": "-------------------------------------  TS MIXIN GENERATOR  ---------------------------------------",
    "prebuild:ts-mixin": "rimraf dist/ts-mixin-generator",
    "build:ts-mixin": "npx tsc --build ./tools/ts-mixin-generator/tsconfig.json",
    "postbuild:ts-mixin": "npx copyfiles --all --flat ./tools/ts-mixin-generator/src/templates/* ./dist/ts-mixin-generator/templates",
    "build:ts-mixin:dev": "npx nodemon --watch ./tools/ts-mixin-generator/src/ --exec npm run build",
    "build:ts-mixin:d": "npx nodemon --watch ./tools/ts-mixin-generator/src/ ./dist/ts-mixin-generator/cli.js",
    "start:ts-mixin": "node ./dist/ts-mixin-generator/cli.js -n test -m post-ts-4 -p mixins",
    "test-cli:ts-mixin": "node ./dist/ts-mixin-generator/cli.js --help",
    "webpack:ts-mixin": "webpack --config tools/ts-mixin-generator/webpack.config.js"
  },
  "dependencies": {
    "@adroit/ng-utils": "file:dist/libs/ng-utils",
    "@angular/animations": "^12.1.0",
    "@angular/cdk": "^12.2.3",
    "@angular/common": "^12.1.0",
    "@angular/compiler": "^12.1.0",
    "@angular/core": "^12.1.0",
    "@angular/flex-layout": "^12.0.0-beta.34",
    "@angular/forms": "^12.1.0",
    "@angular/platform-browser": "^12.1.3",
    "@angular/platform-browser-dynamic": "^12.1.0",
    "@angular/router": "^12.1.0",
    "@nrwl/angular": "12.8.0",
    "colors": "^1.4.0",
    "rxjs": "~6.6.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.1.0",
    "@angular-eslint/eslint-plugin": "~12.3.0",
    "@angular-eslint/eslint-plugin-template": "~12.3.0",
    "@angular-eslint/template-parser": "~12.3.0",
    "@angular/cli": "^12.0.0",
    "@angular/compiler-cli": "^12.1.0",
    "@angular/language-service": "^12.1.0",
    "@nrwl/cli": "12.8.0",
    "@nrwl/cypress": "12.8.0",
    "@nrwl/eslint-plugin-nx": "12.8.0",
    "@nrwl/jest": "12.8.0",
    "@nrwl/linter": "12.8.0",
    "@nrwl/nx-cloud": "12.3.10",
    "@nrwl/tao": "12.8.0",
    "@nrwl/workspace": "12.8.0",
    "@types/jest": "26.0.24",
    "@types/node": "14.14.33",
    "@types/yargs": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "~4.28.3",
    "@typescript-eslint/parser": "~4.28.3",
    "@webpack-cli/generators": "^2.1.0",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.0",
    "copyfiles": "^2.4.1",
    "cypress": "^7.3.0",
    "declaration-bundler-webpack-plugin": "^1.0.3",
    "dotenv": "10.0.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "jest": "27.0.3",
    "jest-preset-angular": "9.0.4",
    "ng-packagr": "^12.1.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.1",
    "rimraf": "^3.0.2",
    "simple-ts-transform": "^1.1.0",
    "terser-webpack-plugin": "^5.2.0",
    "ts-jest": "27.0.3",
    "ts-morph": "~11",
    "ts-node": "~9.1.1",
    "ts-transformer-testing-library": "^1.0.0-alpha.7",
    "tslib": "^2.0.0",
    "typedoc": "^0.21.7",
    "typedoc-webpack-plugin": "^1.1.4",
    "typescript": "~4.3.5",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.51.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.8.0",
    "yargs": "^16.2.0"
  },
  "ts-mixin": {
    "path": "dist/example"
  }
}