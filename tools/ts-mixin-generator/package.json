{
  "name": "ts-mixin-generator",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "rimraf dist",
    "webpack": "webpack",
    "build": "npx tsc && npx copyfiles --all --flat ./src/templates/* ./dist/templates",
    "build:dev": "npx nodemon --watch src/ --exec npm run build",
    "build:d": "npx nodemon --watch src/ ./dist/cli.js",
    "start": "node ./dist/cli.js -n test -m post-ts-4 -p mixins",
    "help": "node ./dist/cli.js --help"
  },
  "bin": {
    "ts-mixin": "./cli.js"
  },
  "keywords": [
    "typescript",
    "ts",
    "mixin",
    "ts mixin",
    "typescript mixin",
    "multiple inheritance"
  ],
  "author": "JohnasBroda",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^14.14.31",
    "@types/yargs": "^16.0.0",
    "@webpack-cli/generators": "^2.1.0",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.0",
    "copyfiles": "^2.4.1",
    "declaration-bundler-webpack-plugin": "^1.0.3",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "yargs": "^16.2.0"
  },
  "dependencies": {
    "colors": "^1.4.0"
  },
  "ts-mixin": {
    "mode": "post-ts-4",
    "path": "mixins"
  }
}
