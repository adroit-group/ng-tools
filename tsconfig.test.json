{
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./test-result",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": ["node_modules/@types"],
    "types": ["node", "jest"],
    "lib": ["es2018", "dom"],
    "paths": {
      "tslib": ["node_modules/tslib/tslib.d.ts"]
    }
  },
  "exclude": ["node_modules/*"]
}
